<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane prefHeight="508.0" prefWidth="679.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mvLab.lab.account.controllers.ReagentAdmissionController">
  <center>
    <TableView prefHeight="369.0" prefWidth="600.0" tableMenuButtonVisible="true">
      <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
      <columns>
          <TableColumn id="postedCol" maxWidth="200.0" minWidth="10.0" prefWidth="72.0" text="V" />
          <TableColumn id="dateCol" maxWidth="1000.0" minWidth="10.0" prefWidth="224.0" text="Date" />
          <TableColumn id="numberCol" maxWidth="2000.0" minWidth="0.0" prefWidth="349.0" text="Number" />
          <TableColumn id="supplierCol" maxWidth="2000.0" prefWidth="208.0" text="Supplier" />
      </columns>
    </TableView>
  </center>
  <stylesheets>
    <URL value="@DarkTheme.css" />
  </stylesheets>
  <top>
    <ToolBar>
      <items>
        <Button fx:id="addButton" mnemonicParsing="false" onMouseClicked="#addButtonClick" text="+" />
        <Button mnemonicParsing="false" text="-" visible="false" />
      </items>
    </ToolBar>
  </top>
</BorderPane>
