<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane prefHeight="508.0" prefWidth="679.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mvLab.lab.accaunt.controllers.ReagentCatalogController">
  <center>
    <TableView prefHeight="369.0" prefWidth="600.0" tableMenuButtonVisible="true">
      <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
      <columns>
            <TableColumn maxWidth="500.0" minWidth="10.0" prefWidth="95.0" />
        <TableColumn maxWidth="1000.0" minWidth="10.0" prefWidth="95.0" text="Date" />
        <TableColumn maxWidth="2000.0" minWidth="0.0" prefWidth="417.0" text="Number" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="202.0" text="Reagent" />
      </columns>
    </TableView>
  </center>
  <stylesheets>
    <URL value="@DarkTheme.css" />
  </stylesheets>
  <top>
    <ToolBar>
      <items>
        <Button fx:id="addButton" mnemonicParsing="false" onMouseClicked="#addButtonClick" text="+" />
        <Button mnemonicParsing="false" text="-" visible="false" />
      </items>
    </ToolBar>
  </top>
</BorderPane>
